

<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap" rel="stylesheet">


<style>

  body {
    font-family: "Ubuntu", sans-serif;
    text-align: center;
    margin: 0px;
  }

  header {
    display: flex;
    justify-content: space-around;
    align-items: center;
    background: #ded415;
    padding: 30px;
    position: relative;

  }

  #intro {
    margin: 0px;
    position: absolute;

  }
  #RA {
    text-align: center;
    margin-left: 15px;
  }
  #games {
    display: flex;
    justify-content: center;
  }
  .game {
    padding: 10px;
    margin: 10px;
    border: 1px solid;
    background: #ded415;
  }
  a {
    text-decoration: none;
    color: black;
  }
  #search_icon {
  margin-left: 10px
}
  
</style>







<html>




  <header id="main">
    <a href='/logout'>Log out</a>
    <h1 id="intro"> hello <%= username %> </h1>
    <div>
      <a href='/mygames'>My games</a>
      <a href="/search">
        <svg id="search_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
          <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0"/>
        </svg>
      </a>
    </div>

  </header>
  <div id="body">
    <h1 id="RA">Recently added</h1>
    <div id="games">
      <% raData.forEach(function(item) { %>
        <% Object.keys(item).forEach(function(gameKey) { %>
          <div class="game">
            <a href="/game/<%= gameKey %>/<%= item[gameKey][0] %>" target="_blank" class="gameName">
              <%= item[gameKey][0] %>
            </a>
            <br>
            <span class="username">
              by: <%= gameKey %>
            </span>

            
          </div>
        
          
          <% }); %>
            
      <% }); %>
    </div>
  </div>





    
  
</html>
  